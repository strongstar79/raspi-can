[Unit]
Description=KOCETI CAN Launcher
Requires=network.target network-onile.target
After=network.target network-online.target

[Service]
Type=simple
WorkingDirectory=/home/cm4-10/config
ExecStart=/home/cm4-10/config/launch.sh
User=cm4-10
Restart=always
RestartSec=5

[Install]
WantedBy=network.target
